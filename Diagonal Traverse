class Solution:
    def findDiagonalOrder(self, mat: List[List[int]]) -> List[int]:

        n= len(mat[0])
        m = len (mat)
        res = defaultdict(list)
        ans =[]

        for i in range(m):
            for j in range(n):
                res[i+j].append(mat[i][j])

        for k,v in res.items():
            if k % 2 == 1:
                ans.extend(v)
            else:
                ans.extend(reversed(v))

        return ans



        
        
        
