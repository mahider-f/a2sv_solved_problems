class RandomizedSet:

    def __init__(self):
        self.num=[]
        self.nums={}

    def insert(self, val: int) -> bool:
        if val in self.nums:
            return False
        self.nums[val]=len(self.num)
        self.num.append(val)
        return True
        

    def remove(self, val: int) -> bool:
        if val not in self.nums:
            return False
        deleted=self.nums[val]
        last=self.num[-1]
        
        self.num[deleted]=last
        self.nums[last]=deleted
        self.num.pop()
        del self.nums[val]
        return True

    def getRandom(self) -> int:
        return random.choice(self.num)


# Your RandomizedSet object will be instantiated and called as such:
# obj = RandomizedSet()
# param_1 = obj.insert(val)
# param_2 = obj.remove(val)
# param_3 = obj.getRandom()
