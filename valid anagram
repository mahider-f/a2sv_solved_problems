class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        count_t = Counter(t)
        count_s = Counter(s)
        flag = False

        if len(s)!=len(t):
            return False
            
        for x,v in count_t.items():
            if (x in t and x in s) and (count_t[x] == count_s[x]):
                flag = True
            else:
                return False

        return flag
