class Solution:
    def pancakeSort(self, arr: List[int]) -> List[int]:

        ans = []
        for target in range(len(arr),1,-1):
            for i in range(target):
                
                if arr[i] == target:
            
                    arr[:i+1] = arr[:i+1][::-1]
                    ans.append(i+1)
                    arr[:target] = arr[:target][::-1]
                    ans.append(target)

                

        return ans
